{
  "wallpaper": {
    "method": "feh",
    "file": "darker_unicat.png"
  },
  "colors": {
    "method": "manual"
  },
  "font_family": "font family JetBrainsMonoNL NFM medium",
  "i3": {"copy": [{
    "from": "config",
    "to": ".config/i3/config"
  }]},
  "polybar": {},
  "nvim": {
    "copy": [{
      "from": "lua/chadrc.lua",
      "to": ".config/nvim/lua/chadrc.lua",
      "overwrite": true
    }],
    "template_dir": "default_configs/nvchad",
    "nvchad_colorscheme": "catppuccin"
  },
  "tmux": {"copy": [{
    "from": "tmux.conf",
    "to": ".tmux.conf"
  }]},
  "rofi": {
    "template_dir": "./themes/i3_catppuccin/rofi/"
  },
  "fish": {
    "feats": ["neofetch"]
  },
  "kitty": {},
  "picom": {
    "template_dir": "./themes/i3_catppuccin/picom/"
  }

}
