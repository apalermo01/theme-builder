{
  "font_family": "font_family JetBrainsMonoNL NFM medium",
  "colors": {
    "method": "pywal"
  },
  "i3": {"copy": [{
    "from": "config",
    "to": ".config/i3/config"
  }]},
  "polybar": {},
  "wallpaper": {
    "method": "feh",
    "file": "trees.jpg"
  },
  "nvim": {
    "template_dir": "default_configs/nvchad",
    "copy": [{
      "from": "lua/chadrc.lua",
      "to": ".config/nvim/lua/chadrc.lua",
      "overwrite": true
    }],
    "nvchad_colorscheme": "everforest"
  },
  "tmux": {},
  "rofi": {
    "template_dir": "./themes/trees/rofi/"
  },
  "fish": {
    "feats": ["run_pywal"],
    "copy": [{
      "from": "config.fish",
      "to": ".config/fish/config.fish"
    }]
  },
  "kitty": {},
  "picom": {
    "template_dir": "./themes/trees/picom/"
  }
}
